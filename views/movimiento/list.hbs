
{{> header}}


<!-- contact section -->

<section class="contact_section layout_padding">
    <div class="custom_heading-container">
        <h3 class=" ">
            Movimientos
        </h3>
    </div>
    
    <div class="container layout_padding2-top">
        <div class="row">
            <div class="col-md-6 mx-auto">
                <table class="table">
                <thead>
                    <tr>
                    {{#each headerlist}}
                        <th scope="row">{{this}}</th>
                    {{/each}}
                    </tr>
                </thead>
                <tbody>
                    {{#each datalist}}
                        <tr>
                        <td>{{this.id_movimiento}}</td>
                        <td>{{this.placa}}</td>
                        <td>{{this.estado}}</td>
                        <td>{{this.createdAt}}</td>
                        </tr>
                    {{/each}}

                </tbody>
                </table>
            </div>
        </div>
    </div>
</section>
