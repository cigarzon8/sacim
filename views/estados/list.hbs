{{> layout}}
{{> nav}}
<!-- contact section -->

<section class="contact_section layout_padding">
    {{#each datalist}}



        <div class="custom_heading-container">
            <h3 class=" ">
                {{this.nombre}}
            </h3>
        </div>


        <div class="container layout_padding2-top">
            <div class="row">
                <div class="col-md-6 mx-auto">


                    <table class="table">
                    <thead>
                        <tr>
                        {{#each  ../headerlist}}
                            <th scope="row">{{this}}</th>
                        {{/each}}
                        </tr>
                    </thead>
                    <tbody>
                        {{#each this.datos}}
                            <tr>
                            <td>{{this.id}}</td>
                            <td>{{this.nombre_estado}}</td>
                            </tr>
                        {{/each}}

                    </tbody>
                    </table>



                </div>
            </div>

        </div>
    {{/each}}




</section>


{{> footer}}